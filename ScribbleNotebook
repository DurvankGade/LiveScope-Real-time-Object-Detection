      "code": "(async () => {\n  // Load required libraries\n  await loadScript('https://cdn.jsdelivr.net/npm/@tensorflow/tfjs');\n  await loadScript('https://cdn.jsdelivr.net/npm/@tensorflow-models/blazeface');\n  await loadScript('https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.7.0/chart.min.js');\n\n  function loadScript(url) {\n    return new Promise((resolve, reject) => {\n      const script = document.createElement('script');\n      script.src = url;\n      script.onload = resolve;\n      script.onerror = reject;\n      document.head.appendChild(script);\n    });\n  }\n\n  // Create container with modern styling\n  const container = document.createElement('div');\n  container.style.fontFamily = 'Arial, sans-serif';\n  container.style.maxWidth = '1200px';\n  container.style.margin = '0 auto';\n  container.style.padding = '20px';\n  container.style.backgroundColor = '#f5f5f5';\n  container.style.borderRadius = '10px';\n  container.style.boxShadow = '0 4px 6px rgba(0, 0, 0, 0.1)';\n  document.body.appendChild(container);\n\n  // Title and description\n  const header = document.createElement('div');\n  header.innerHTML = `\n    <h1 style=\"color: #2c3e50; text-align: center; margin-bottom: 10px;\">Smart Attention Monitor</h1>\n    <p style=\"color: #7f8c8d; text-align: center; margin-bottom: 20px;\">\n      Real-time attention tracking with advanced analytics\n    </p>\n  `;\n  container.appendChild(header);\n\n  // Create main content wrapper\n  const contentWrapper = document.createElement('div');\n  contentWrapper.style.display = 'grid';\n  contentWrapper.style.gridTemplateColumns = '1fr 1fr';\n  contentWrapper.style.gap = '20px';\n  container.appendChild(contentWrapper);\n\n  // Left column - Video feed and controls\n  const leftColumn = document.createElement('div');\n  leftColumn.style.backgroundColor = '#fff';\n  leftColumn.style.padding = '20px';\n  leftColumn.style.borderRadius = '8px';\n  contentWrapper.appendChild(leftColumn);\n\n  // Video container\n  const videoContainer = document.createElement('div');\n  videoContainer.style.position = 'relative';\n  videoContainer.style.width = '100%';\n  videoContainer.style.marginBottom = '20px';\n  leftColumn.appendChild(videoContainer);\n\n  const video = document.createElement('video');\n  video.id = 'video';\n  video.width = 640;\n  video.height = 480;\n  video.autoplay = true;\n  video.style.width = '100%';\n  video.style.borderRadius = '8px';\n  videoContainer.appendChild(video);\n\n  const canvas = document.createElement('canvas');\n  canvas.id = 'overlay';\n  canvas.width = 640;\n  canvas.height = 480;\n  canvas.style.position = 'absolute';\n  canvas.style.top = '0';\n  canvas.style.left = '0';\n  canvas.style.width = '100%';\n  canvas.style.height = '100%';\n  videoContainer.appendChild(canvas);\n\n  // Controls\n  const controls = document.createElement('div');\n  controls.style.display = 'flex';\n  controls.style.gap = '10px';\n  controls.style.marginBottom = '20px';\n  leftColumn.appendChild(controls);\n\n  const startButton = document.createElement('button');\n  startButton.textContent = 'Start Monitoring';\n  startButton.style.padding = '10px 20px';\n  startButton.style.backgroundColor = '#2ecc71';\n  startButton.style.color = 'white';\n  startButton.style.border = 'none';\n  startButton.style.borderRadius = '5px';\n  startButton.style.cursor = 'pointer';\n  controls.appendChild(startButton);\n\n  const stopButton = document.createElement('button');\n  stopButton.textContent = 'Stop';\n  stopButton.style.padding = '10px 20px';\n  stopButton.style.backgroundColor = '#e74c3c';\n  stopButton.style.color = 'white';\n  stopButton.style.border = 'none';\n  stopButton.style.borderRadius = '5px';\n  stopButton.style.cursor = 'pointer';\n  controls.appendChild(stopButton);\n\n  // Right column - Stats and analytics\n  const rightColumn = document.createElement('div');\n  rightColumn.style.backgroundColor = '#fff';\n  rightColumn.style.padding = '20px';\n  rightColumn.style.borderRadius = '8px';\n  contentWrapper.appendChild(rightColumn);\n\n  // Status indicators\n  const statusPanel = document.createElement('div');\n  statusPanel.style.marginBottom = '20px';\n  rightColumn.appendChild(statusPanel);\n\n  const attentionStatus = document.createElement('div');\n  attentionStatus.id = 'status';\n  attentionStatus.style.fontSize = '18px';\n  attentionStatus.style.fontWeight = 'bold';\n  attentionStatus.style.marginBottom = '10px';\n  attentionStatus.textContent = 'Attention Status: Unknown';\n  statusPanel.appendChild(attentionStatus);\n\n  // Statistics\n  const stats = document.createElement('div');\n  stats.style.display = 'grid';\n  stats.style.gridTemplateColumns = '1fr 1fr';\n  stats.style.gap = '10px';\n  stats.style.marginBottom = '20px';\n  rightColumn.appendChild(stats);\n\n  // Create stat boxes\n  const createStatBox = (title, value) => {\n    const box = document.createElement('div');\n    box.style.backgroundColor = '#f8f9fa';\n    box.style.padding = '15px';\n    box.style.borderRadius = '5px';\n    box.style.textAlign = 'center';\n    box.innerHTML = `\n      <div style=\"color: #7f8c8d; font-size: 14px;\">${title}</div>\n      <div style=\"color: #2c3e50; font-size: 24px; font-weight: bold;\">${value}</div>\n    `;\n    return box;\n  };\n\n  const timeBox = createStatBox('Session Time', '00:00');\n  const attentiveBox = createStatBox('Attention Score', '0%');\n  stats.appendChild(timeBox);\n  stats.appendChild(attentiveBox);\n\n  // Add chart\n  const chartContainer = document.createElement('div');\n  chartContainer.style.marginTop = '20px';\n  rightColumn.appendChild(chartContainer);\n\n  const chartCanvas = document.createElement('canvas');\n  chartCanvas.id = 'attentionChart';\n  chartContainer.appendChild(chartCanvas);\n\n  // Initialize chart\n  const ctx = chartCanvas.getContext('2d');\n  const attentionData = {\n    labels: [],\n    datasets: [{\n      label: 'Attention Level',\n      data: [],\n      borderColor: '#3498db',\n      fill: false\n    }]\n  };\n\n  const chart = new Chart(ctx, {\n    type: 'line',\n    data: attentionData,\n    options: {\n      responsive: true,\n      scales: {\n        y: {\n          beginAtZero: true,\n          max: 100\n        }\n      }\n    }\n  });\n\n  // Variables for tracking\n  let isMonitoring = false;\n  let startTime = null;\n  let attentiveTime = 0;\n  let totalTime = 0;\n\n  // Start/Stop monitoring\n  startButton.addEventListener('click', () => {\n    isMonitoring = true;\n    startTime = Date.now();\n    startButton.disabled = true;\n    stopButton.disabled = false;\n  });\n\n  stopButton.addEventListener('click', () => {\n    isMonitoring = false;\n    startButton.disabled = false;\n    stopButton.disabled = true;\n  });\n\n  // Initialize video stream\n  const videoElement = document.getElementById('video');\n  const canvasElement = document.getElementById('overlay');\n  const statusElement = document.getElementById('status');\n  const ctx2d = canvasElement.getContext('2d');\n\n  try {\n    const stream = await navigator.mediaDevices.getUserMedia({ video: true });\n    videoElement.srcObject = stream;\n  } catch (error) {\n    console.error('Error accessing camera:', error);\n    alert('Error accessing camera. Please ensure you have allowed permissions.');\n    return;\n  }\n\n  const model = await blazeface.load();\n\n  // Update timer\n  function updateTimer() {\n    if (isMonitoring && startTime) {\n      const elapsed = Math.floor((Date.now() - startTime) / 1000);\n      const minutes = Math.floor(elapsed / 60).toString().padStart(2, '0');\n      const seconds = (elapsed % 60).toString().padStart(2, '0');\n      timeBox.querySelector('div:last-child').textContent = `${minutes}:${seconds}`;\n    }\n  }\n\n  // Update attention score\n  function updateAttentionScore(isAttentive) {\n    if (isMonitoring) {\n      totalTime++;\n      if (isAttentive) attentiveTime++;\n      const score = Math.round((attentiveTime / totalTime) * 100);\n      attentiveBox.querySelector('div:last-child').textContent = `${score}%`;\n\n      // Update chart\n      const timestamp = new Date().toLocaleTimeString();\n      attentionData.labels.push(timestamp);\n      attentionData.datasets[0].data.push(score);\n\n      // Keep last 10 data points\n      if (attentionData.labels.length > 10) {\n        attentionData.labels.shift();\n        attentionData.datasets[0].data.shift();\n      }\n      chart.update();\n    }\n  }\n\n  async function detectAttention() {\n    const returnTensors = false;\n    const predictions = await model.estimateFaces(videoElement, returnTensors);\n    \n    ctx2d.clearRect(0, 0, canvasElement.width, canvasElement.height);\n    \n    let isAttentive = false;\n    \n    if (predictions.length > 0) {\n      predictions.forEach(prediction => {\n        const start = prediction.topLeft;\n        const end = prediction.bottomRight;\n        const size = [end[0] - start[0], end[1] - start[1]];\n\n        // Draw face box with gradient\n        const gradient = ctx2d.createLinearGradient(start[0], start[1], end[0], end[1]);\n        gradient.addColorStop(0, 'rgba(46, 204, 113, 0.5)');\n        gradient.addColorStop(1, 'rgba(52, 152, 219, 0.5)');\n        \n        ctx2d.fillStyle = gradient;\n        ctx2d.fillRect(start[0], start[1], size[0], size[1]);\n        \n        // Draw border\n        ctx2d.strokeStyle = '#2ecc71';\n        ctx2d.lineWidth = 2;\n        ctx2d.strokeRect(start[0], start[1], size[0], size[1]);\n\n        const headCenterX = (start[0] + end[0]) / 2;\n        const headCenterY = (start[1] + end[1]) / 2;\n        const faceWidth = end[0] - start[0];\n\n        if (headCenterX > canvasElement.width * 0.35 && \n            headCenterX < canvasElement.width * 0.65 && \n            faceWidth > canvasElement.width * 0.2) {\n          statusElement.textContent = 'Attention Status: Focused 👍';\n          statusElement.style.color = '#2ecc71';\n          isAttentive = true;\n        } else {\n          statusElement.textContent = 'Attention Status: Distracted ⚠️';\n          statusElement.style.color = '#e74c3c';\n        }\n      });\n    } else {\n      statusElement.textContent = 'Attention Status: No Face Detected ❌';\n      statusElement.style.color = '#7f8c8d';\n    }\n\n    updateTimer();\n    updateAttentionScore(isAttentive);\n    requestAnimationFrame(detectAttention);\n  }\n\n  detectAttention();\n})();\n",

ight: '400',\n        controls: 'controls'\n    });\n    videoContainer.appendChild(video);\n    container.appendChild(videoContainer);\n\n    // Create status message element\n    const statusMessage = createElement('p', {}, { color: '#666' });\n    container.appendChild(statusMessage);\n\n    // Create video results container\n    const videoResultsContainer = createElement('div', {}, {\n        width: '100%',\n        maxWidth: '600px',\n        marginTop: '20px'\n    });\n    container.appendChild(videoResultsContainer);\n\n    // Function to update status message\n    const updateStatus = (message) => {\n        statusMessage.textContent = message;\n    };\n\n    // Function to fetch YouTube videos based on the user query\n    const fetchYouTubeVideos = async (query) => {\n        const apiKey = 'AIzaSyAsxnp7YgiJIfZWoaYkRFWIatgcrjBZh18'; // Replace with your YouTube API key\n        const url = `https://www.googleapis.com/youtube/v3/search?part=snippet&q=${encodeURIComponent(query)}&type=video&key=${apiKey}`;\n        const response = await fetch(url);\n        if (!response.ok) {\n            throw new Error('Network response was not ok');\n        }\n        const data = await response.json();\n        return data.items; // Return the list of video items\n    };\n\n    // Function to generate video from text\n    const generateVideo = async (text) => {\n        const canvas = document.createElement('canvas');\n        const ctx = canvas.getContext('2d');\n        canvas.width = 600;\n        canvas.height = 400;\n        const stream = canvas.captureStream(30); // 30 FPS\n\n        const mediaRecorder = new MediaRecorder(stream, {\n            mimeType: 'video/webm; codecs=vp9'\n        });\n\n        const chunks = [];\n        mediaRecorder.ondataavailable = (e) => chunks.push(e.data);\n        mediaRecorder.onstop = () => {\n            const blob = new Blob(chunks, { type: 'video/webm' });\n            video.src = URL.createObjectURL(blob);\n            videoContainer.style.display = 'block';\n            updateStatus('Video generated successfully!');\n            video.play(); // Automatically play the generated video\n        };\n\n        mediaRecorder.start();\n        let frame = 0;\n        const animate = () => {\n            ctx.fillStyle = '#000000';\n            ctx.fillRect(0, 0, canvas.width, canvas.height);\n            ctx.fillStyle = '#ffffff';\n            ctx.font = '30px Arial';\n            ctx.textAlign = 'center';\n            ctx.textBaseline = 'middle';\n\n            const y = canvas.height / 2 + Math.sin(frame / 30) * 20;\n            ctx.fillText(text, canvas.width / 2, y);\n            frame++;\n\n            if (frame < 90) { // Change 90 to increase or decrease duration\n                requestAnimationFrame(animate);\n            } else {\n                mediaRecorder.stop();\n            }\n        };\n        animate();\n    };\n\n    // Click event for convert button\n    convertButton.onclick = async () => {\n        const text = textInput.value.trim();\n        if (!text) {\n            updateStatus('Please enter some text first!');\n            return;\n        }\n\n        updateStatus('Generating video...');\n        convertButton.disabled = true;\n\n        try {\n            await generateVideo(text);\n            const videos = await fetchYouTubeVideos(text);\n            displayVideoResults(videos);\n        } catch (error) {\n            updateStatus('Error: ' + error.message);\n            console.error('Error:', error);\n        } finally {\n            convertButton.disabled = false;\n        }\n    };\n\n    // Function to display fetched YouTube videos\n    const displayVideoResults = (videos) => {\n        videoResultsContainer.innerHTML = ''; // Clear previous results\n        videos.forEach(videoItem => {\n            const videoElement = createElement('a', {\n                href: `https://www.youtube.com/watch?v=${videoItem.id.videoId}`,\n                target: '_blank'\n            }, {\n                display: 'block',\n                padding: '8px',\n                margin: '10px 0',\n                backgroundColor: '#f0f0f0',\n                borderRadius: '4px',\n                textDecoration: 'none',\n                color: '#007bff',\n                border: '1px solid #ccc'\n            });\n            videoElement.innerText = videoItem.snippet.title;\n            videoResultsContainer.appendChild(videoElement);\n        });\n    };\n\n    // Handle Enter key in text input\n    textInput.addEventListener('keypress', (e) => {\n        if (e.key === 'Enter') {\n            convertButton.click();\n        }\n    });\n\n    // Initial status\n    updateStatus('Enter text and click Convert or press Enter to generate a video.');\n})();",
